name: function(module) {
                    switch(module){
                        case "a":
                            return "hahahah";
                        case "b":
                            return "bbbbbbb";
                        case "c":
                            return "cccccc";
                        case "d":
                            return "ddđ";
                        case "e":
                            return "eeeee";
                        case "f":
                            return "fffff";
                        case "g":
                            return "ggggg";
                     }
                }
                
=>

func (module) {
	return {
		"te": "haha"
	}[module]
}

có thể lưu trữ function trong 1 object

const obj = {
	name: 'Quan',  -> property
	greet: function greet (){     -> method
		console.log('haha');
	}	
}


Example:
switch (section.id) {
                        case 'traffic': {
                            self._fetchTrafficData(section.id, locationId, self.floorSelected._id, timeRange);
                            return;
                        }
                        case 'sales': {
                            self._fetchSalesData(section.id, locationId, self.floorSelected._id, timeRange);
                            return;
                        }
                        case 'demographics': {
                            self._fetchDemographicsData(section.id, locationId, self.floorSelected._id, timeRange);
                            return;
                        }
                        case 'heatmap': {
                            self._fetchHeatmapData(section.id, locationId, self.floorSelected._id, timeRange);
                            return;
                        }
                        case 'staff': {
                            self._fetchStaffData(section.id, locationId, self.floorSelected._id, timeRange);
                            return;
                        }
                        default: {}
                    }

====>
return {
       traffic: self._fetchTrafficData,
       sales: self._fetchSalesData,
       demographics: self._fetchDemographicsData,
       heatmap: self._fetchHeatmapData,
       staff: self._fetchStaffData,
}[section.id](section.id, locationId, self.floorSelected._id, timeRange);
